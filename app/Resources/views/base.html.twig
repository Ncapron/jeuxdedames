
<!DOCTYPE html>
<html>

<!-- Website made by Nicolas Capron && Pol Soyez -->

    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Les jeux de dames{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <meta name="viewport" content="width=device-width" />

        {% block stylesheets %}

            <!-- Latest compiled and minified CSS -->
            <link rel="stylesheet" href="{{ asset ('bundles/cms/css/bootstrap.min.css') }}"/>
            <!-- Optional theme -->
            <link rel="stylesheet" href="{{ asset ('bundles/cms/css/bootstrap-theme.min.css') }}"/>

            <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Damion" />
            <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans"/>
            <!-- Appel feuille de style -->
            <link rel="stylesheet" href="{{ asset ('bundles/cms/css/style.css') }}"/>

        {% endblock %}
    </head>

    <body>

        <!-- ******** NAVBAR ******* -->
        <header>
            <div class="head_content">
                <a class="menu_gauche menu" href="{{ path('cms_homepage') }}">Jeux de Dames</a>
                <a class="menu_droite menu" href="{{ path('user_archive') }}">Archives</a>
                <a class="menu_droite menu" href="{{ path('user_presse') }}">Presse</a>
                <a class="menu_droite menu" href="{{ path('user_partenaire') }}">Partenaires</a>
                <a class="menu_droite menu" href="{{ path('user_commercant') }}">Commer√ßants</a>
                <a class="menu_droite menu" href="{{ path('user_artiste') }}">Artistes</a>
            </div>
        </header>

        {% block body %}{% endblock %}

        <footer>
            <!-- ******** FOOTER ******** -->
            <div id="">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 reseaux">
                        <h5>Jeux de Dames</h5>
                        <p class="txt-fin">
                            <a href="#"><i class="ion-social-twitter"></i></a>
                            <a href="#"><i class="ion-social-instagram"></i></a>
                            <a href="#"><i class="ion-social-dribbble"></i></a>
                        </p>
                    </div>
                </div>
            </div>
        </footer>

        {% block javascripts %}

            <script src="{{ asset ('bundles/cms/js/jquery-3.0.0.min.js') }}" ></script>
            <script src="{{ asset ('bundles/cms/js/jquery-2.0.0.min.js') }}" ></script>

            <!-- Latest compiled and minified JavaScript -->
            <script src="{{ asset ('bundles/cms/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset ('bundles/cms/js/masonry.pkgd.js') }}"></script>

            <script type="text/javascript" src="{{ asset('bundles/cms/js/main.js') }}"></script>

        {% endblock %}

    </body>
</html>
