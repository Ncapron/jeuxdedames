{% extends 'base.html.twig' %}

{% block body %}
<div class="partenaires">

    {% for partenaire in partenaires %}
        <div class="col-md-3">
            <img src="{{ asset('uploads/imgcms/') }}{{ partenaire.image }}" >
            <a href="{{ path('partenaire_edit', { 'id': partenaire.id }) }}">edit</a>
        </div>
    {% endfor %}
    <div>
        <a href="{{ path('partenaire_new') }}">Create a new entry</a>
    </div>
{% endblock %}